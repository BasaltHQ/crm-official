# Voice Connector Streaming Consumer (.env)
# Copy to .env and fill values.

# Gateway bridge
GATEWAY_URL=wss://ledger1-gateway.yellowplant-475b29bf.eastus2.azurecontainerapps.io/ingest
GATEWAY_SECRET=changeme

# Call/session identity (use SMA/VC transaction id if available)
CALL_ID=demo-call-uuid

# Upstream audio format from VC -> Gateway
IN_ENCODING=mulaw
IN_SAMPLE_RATE=8000

# Downstream audio format to inject back into VC
OUT_ENCODING=mulaw
OUT_SAMPLE_RATE=8000

# Adapter mode: 'bidi' for Voice Connector bidirectional streaming, 'kvs' for Kinesis stream consumer
MODE=bidi

# AWS region and resource ids (used by adapters when implemented)
AWS_REGION=us-west-2
VC_ID=
KVS_STREAM_ARN=
